; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="Pocket Galaxy"
run/main_scene="res://screens/title_screen/title_screen.tscn"
config/icon="res://icon.png"
config/version=0.1

[autoload]

global="*res://global_scripts/global.gd"
Navigator="*res://global_scripts/navigator.gd"

[display]

window/size/width=480
window/size/height=270
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

[gui]

theme/custom="res://themes/main_theme.tres"
theme/custom_font="res://fonts/default_font.tres"

[physics]

2d/default_gravity=0
2d/default_linear_damp=0.5
2d/default_angular_damp=0

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/shadows/filter_mode=0
quality/filters/anisotropic_filter_level=1
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
